      "title": "Limit Replacements",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource:Prometheus}"},
      "fieldConfig": {
        "defaults": {"unit": "milliseconds"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "id": 5,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(exec_limit_ttl_ms_sum{app=\"bot-trading\"}[15m])) / sum(rate(exec_limit_ttl_ms_count{app=\"bot-trading\"}[15m]))",
