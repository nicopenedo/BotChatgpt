      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "sum(rate(exec_queueTime_ms_sum{app=\"bot-trading\"}[5m])) / sum(rate(exec_queueTime_ms_count{app=\"bot-trading\"}[5m]))",
          "legendFormat": "avg queue"
        }
      ],
      "title": "Order Queue Time",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource:Prometheus}"},
      "fieldConfig": {
        "defaults": {"unit": "none"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
