{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "datasource",
          "uid": "grafana"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": {"type": "prometheus", "uid": "${datasource:Prometheus}"},
      "fieldConfig": {
        "defaults": {"unit": "bps"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "anomaly_metric{app=\"bot-trading\",metric=\"slippage_bps\"}",
          "legendFormat": "slippage {{symbol}}"
        },
        {
          "expr": "anomaly_metric{app=\"bot-trading\",metric=\"spread_bps\"}",
          "legendFormat": "spread {{symbol}}"
        }
      ],
      "title": "Slippage vs Spread",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource:Prometheus}"},
      "fieldConfig": {
        "defaults": {"unit": "percentunit"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(orders_filled_total{app=\"bot-trading\"}[15m])) / sum(rate(orders_sent_total{app=\"bot-trading\"}[15m]))",
          "legendFormat": "fill rate"
        }
      ],
      "title": "Fill Rate",
      "type": "timeseries"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource:Prometheus}"},
      "fieldConfig": {
        "defaults": {"unit": "milliseconds"},
