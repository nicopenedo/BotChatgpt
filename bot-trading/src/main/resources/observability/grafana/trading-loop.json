{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "datasource",
          "uid": "grafana"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource:Prometheus}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "milliseconds"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(scheduler_candle_duration_ms_seconds_bucket{app=\"bot-trading\"}[5m])) by (le)) * 1000",
          "legendFormat": "p95"
        },
        {
          "expr": "sum(rate(scheduler_candle_duration_ms_seconds_sum{app=\"bot-trading\"}[5m])) * 1000 / sum(rate(scheduler_candle_duration_ms_seconds_count{app=\"bot-trading\"}[5m]))",
          "legendFormat": "avg"
        }
      ],
      "title": "E2E Candle Latency",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource:Prometheus}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "milliseconds"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "scheduler_candle_backlog_ms{app=\"bot-trading\"}",
          "legendFormat": "{{symbol}}"
        }
      ],
      "title": "Scheduler Backlog",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource:Prometheus}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "sum(queue_depth{app=\"bot-trading\",symbol=\"global\"})",
          "legendFormat": "global"
        },
        {
          "expr": "sum(queue_depth{app=\"bot-trading\",symbol!~=\"global\"}) by (symbol)",
          "legendFormat": "{{symbol}}"
        }
      ],
      "title": "Throttle Queue Depth",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource:Prometheus}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(binance_api_requests_total{app=\"bot-trading\",status=\"rate_limited\"}[5m]))",
          "legendFormat": "429 rate"
        },
        {
          "expr": "sum(rate(binance_api_requests_total{app=\"bot-trading\",status=\"server_error\"}[5m]))",
          "legendFormat": "5xx rate"
        }
      ],
      "title": "API Errors",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource:Prometheus}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "id": 5,
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(throttle_rejections_total{app=\"bot-trading\"}[5m]))",
          "legendFormat": "rejections/min"
        }
      ],
      "title": "Throttle Rejections",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource:Prometheus}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "id": 6,
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "increase(risk_stopouts_total{app=\"bot-trading\"}[6h])",
          "legendFormat": "stopouts"
        }
      ],
      "title": "Risk Guard Events (6h)",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["bot-trading", "trading"],
  "templating": {"list": []},
